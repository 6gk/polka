def_key "p"
	add_item_to_playlist

def_key "space"
	pause

def_key "right"
	seek_forward

def_key "left"
	seek_backward

def_key "j"
	move_selected_items_down

def_key "k"
	move_selected_items_up

def_key "="
	volume_up

def_key "-"
	volume_down

def_key "delete"
	delete_playlist_items

def_key "s"
	shuffle

def_key "n"
	next

def_key "N"
	previous
