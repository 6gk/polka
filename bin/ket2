#!/bin/sh
#
# Credit to this guy
#   https://www.reddit.com/r/minimalcatart/comments/cvqukg/smol_ket/
#


#bb="\033[48;05;232m"
#fb="\033[38;05;232m"

#bg="\033[48;05;242m"
#fg="\033[38;05;242m"

#bw="\033[48;05;252m"
#fw="\033[38;05;252m"

#br="\033[48;05;161m"
#fr="\033[38;05;161m"

#by="\033[48;05;178m"

bb=""
fb=""

fg="\033[97m"
bg="\033[107m"

by="\033[43m"

br="\033[41m"
fr="\033[31m"

bw="\033[107m"
fw="\033[97m"

c="\033[0m"

# █ ▄ ▀


printf "
 $fg▄$c  $fg▄ $c
$fg ▀$fw█$fg▀█ $c
 $c$fw▀$by▀$br▀$c$fr$bw▀$c$fw█$fg$c$fw▄▄▄$c$fw   ▄
  $fw$bw▀$c$fw▀$bw▀$bw▀$bg▀$c$fw██$fw▀▄▄▀
  $fw▀ $fw▀ $fg▀ ▀$fg$c

"
